MIGRATE(
    SOURCE(TABLE, actor) -> DEST(TABLE, actor) [
        SETTINGS(CREATE_MISSING_TABLES=TRUE,IGNORE_CONSTRAINTS=TRUE,COPY_COLUMNS=MAP_ONLY),
