        SETTINGS(CREATE_MISSING_COLUMNS=TRUE,CREATE_MISSING_TABLES=TRUE,COPY_COLUMNS=MAP_ONLY),
        MAP(user_id -> user_id, id -> id, total -> total)
    ]
