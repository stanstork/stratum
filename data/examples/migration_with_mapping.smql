CONNECTIONS (
    SOURCE MYSQL "mysql://user:password@localhost:3306/testdb",
    DESTINATION POSTGRES "postgres://user:password@localhost:5432/testdb"
);

MIGRATE (
    invoices -> statement,
    orders -> orders,
    products -> goods
)
WITH SETTINGS (
    INFER_SCHEMA = TRUE
);

MAP (
    products (
        id -> product_id
    ),
    orders (
        total -> ammount
    )
);