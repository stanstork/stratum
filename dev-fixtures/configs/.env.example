# Example .env file for SMQL environment variables
# Copy this file to .env and fill in your actual values

# Source database connection (MySQL)
SOURCE_DB_URL=mysql://user:password@localhost:3306/testdb

# Target database connection (PostgreSQL)
TARGET_DB_URL=postgres://user:password@localhost:5432/testdb

# Optional: Database connection pool size (defaults to 20 if not set)
DB_POOL_SIZE=50

# Optional: Batch size for migrations (defaults to 1000 if not set)
BATCH_SIZE=500

# Optional: Default region for customer data
DEFAULT_REGION=US
